{
  "name": "multiplan",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "makemigration": "npx sequelize-cli migration:create"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.1.1"
  },
  "_moduleAliases": {
    "@root": ".",
    "@models": "models/",
    "@services": "app/services/",
    "@controllers": "app/controllers/",
    "@routes": "routes/"
  },
  "devDependencies": {
    "sequelize-auto-migrations": "^1.0.3"
  }
}
